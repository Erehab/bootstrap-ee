{
  "name": "@erehab/bootstrap-ee",
  "version": "2.2.0",
  "private": true,
  "main": "js/bs-ee.js",
  "sass": "src/scss/bs-ee.scss",
  "style": "css/bs-ee.css",
  "scripts": {
    "css": "npm-run-all css-compile css-prefix css-minify",
    "css-compile": "sass --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css src/scss/:css/",
    "css-minify": "npm-run-all --aggregate-output --parallel css-minify-*",
    "css-minify-main": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output css/ --batch --batch-suffix \".min\" \"css/*.css\" \"!css/*.min.css\" \"!css/*rtl*.css\"",
    "css-prefix": "npm-run-all --aggregate-output --parallel css-prefix-*",
    "css-prefix-main": "postcss --config build/postcss.config.js --replace \"css/*.css\" \"!css/*.rtl*.css\" \"!css/*.min.css\"",
    "copy-fonts": "mkdir -p webfonts && cp node_modules/@fortawesome/fontawesome-pro/webfonts/*.woff2 webfonts/",
    "dist": "npm-run-all --aggregate-output --parallel css copy-fonts",
    "publish-dist": "bash build/publish-dist.sh",
    "build-vite": "vite build -c vite.config.ts",
    "lint:ts": "eslint -c .eslintrc.cjs --ext .ts src/ts",
    "start": "browser-sync start --server . --port 7788 --startPath /htmltest/ --watch 'css/*.css' --watch 'js/*.js' --watch 'htmltest/*.html' --watch '*.html' --no-notify",
    "watch:css": "sass --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css --watch src/scss/:css/",
    "dev": "npm-run-all --parallel watch:css start"
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^7.2.0",
    "bootstrap": "^5.3.2",
    "datatables.net-autofill-bs5": "^2.7.1",
    "datatables.net-bs5": "^2.3.7",
    "datatables.net-buttons-bs5": "^3.2.6",
    "datatables.net-colreorder-bs5": "^2.1.2",
    "datatables.net-fixedcolumns-bs5": "^5.0.5",
    "datatables.net-fixedheader-bs5": "^4.0.6",
    "datatables.net-keytable-bs5": "^2.12.2",
    "datatables.net-responsive-bs5": "^3.0.8",
    "datatables.net-rowgroup-bs5": "^1.6.0",
    "datatables.net-rowreorder-bs5": "^1.5.1",
    "datatables.net-scroller-bs5": "^2.4.3",
    "datatables.net-searchbuilder-bs5": "^1.8.4",
    "datatables.net-searchpanes-bs5": "^2.3.5",
    "datatables.net-select-bs5": "^3.1.3",
    "datatables.net-staterestore-bs5": "^1.4.3"
  },
  "devDependencies": {
    "@fontsource/nunito": "^5.2.7",
    "@types/bootstrap": "^5.2.10",
    "@types/datatables.net": "^1.10.28",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "autoprefixer": "^10.4.8",
    "browser-sync": "^3.0.4",
    "clean-css-cli": "^5.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.0",
    "rtlcss": "^4.0.0",
    "sass": "^1.52.3",
    "typescript": "^5.3.2",
    "vite": "^5.0.5"
  },
  "files": [
    "css/*.{css,map}",
    "js/*.{js,mjs,map}",
    "webfonts/*.woff2",
    "src/{ts,scss}/**/*.{ts,scss}"
  ]
}
